{
  "containerDefinitions": [
    {
      "name": "xlr8-frontend",
      "image": "363187237379.dkr.ecr.us-east-1.amazonaws.com/xlr8-frontend",
      "portMappings": [
        {
          "name": "xlr8-frontend-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "family": "frontend",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
